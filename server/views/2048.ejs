<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <meta name="viewport" content="width=device-width, target-densitydpi=160dpi, initial-scale=1.0, maximum-scale=1, user-scalable=no, minimal-ui">

    <title>2048</title>

    <!-- 2048 Game CSS -->
    <link href="/stylesheets/2048.css" rel="stylesheet" type="text/css">
</head>
<body>
<div class="container">
    <div class="heading">
        <h1 class="title">2048</h1>

        <div class="scores-container">
            <div class="score-container">0</div>
            <div class="best-container">0</div>
        </div>
    </div>

    <div class="above-game">
        <p class="game-intro">Join the numbers and get to the <strong>2048 tile!</strong></p>
    </div>

    <div class="game-container">
        <div class="game-message">
            <p></p>
        </div>

        <div class="grid-container">
            <div class="grid-row">
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
            </div>
            <div class="grid-row">
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
            </div>
            <div class="grid-row">
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
            </div>
            <div class="grid-row">
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
                <div class="grid-cell"></div>
            </div>
        </div>

        <div class="tile-container">

        </div>
    </div>

</div>

<script src="/js/games/2048/bind_polyfill.js"></script>
<script src="/js/games/2048/classlist_polyfill.js"></script>
<script src="/js/games/2048/animframe_polyfill.js"></script>
<script src="/js/games/2048/keyboard_input_manager.js"></script>
<script src="/js/games/2048/html_actuator.js"></script>
<script src="/js/games/2048/grid.js"></script>
<script src="/js/games/2048/tile.js"></script>
<script src="/js/games/2048/local_storage_manager.js"></script>
<script src="/js/games/2048/game_manager.js"></script>
<script src="/js/games/2048/application.js"></script>
</body>
</html>
